<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="CarAutoStarter.MainPage"
             Title="Navigation Home">

    <ContentPage.ToolbarItems>
        <ToolbarItem x:Name="settingsPage" Text="Settings" Order="Secondary" Clicked="settingsPage_Clicked"/>
        <ToolbarItem x:Name="addUserPage" Text="Add User" Order="Secondary" Clicked="addUserPage_Clicked"/>
    </ContentPage.ToolbarItems>

    <StackLayout>
        <Button Text="Car1"/>
        <Button Text="Car2"/>
        <Button Text="Car3"/>
        <Button Text="Car4"/>
        <Button Text="Car5"/>
    </StackLayout>

</ContentPage>
